<h1>Team Index</h1>
<div class="container">
  <div class="row mb-3 p-3">
    <% @teams.each do |team| %>
    <div class="card text-bg-primary mb-3 col-3" class="custom-card" style="width: 18rem; height: 12rem;">
      <div class="card-body" >
        <h5 class="card-title"><%= team.name %></h5>
        <a href="#" class="stretched-link"></a>
      </div>
    </div>
    <% end %>
  </div>
</div>
