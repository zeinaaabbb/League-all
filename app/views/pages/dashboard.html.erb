<div class="row" id="dashboard">
  <div>
    <%= render "shared/sidebar", league: nil %>
  </div>
  <div class="column" style="padding: 0px; grid-template-columns: 5fr;">
    <div>
      <%= render "shared/navbar" %>
    </div>
    <div id="dashboard-content" style="padding:20px">
      <h2>Dashboard</h2>
      <br>
      <div id="dashboard-content2" style="padding:20px">
        <div class="tab-content" id="v-pills-tabContent">
          <div class="tab-pane fade show active" id="v-pills-leagues" role="tabpanel" aria-labelledby="v-pills-leagues-tab">
            <%= render "leagues"%>
          </div>
          <div class="tab-pane fade" id="v-pills-teams" role="tabpanel" aria-labelledby="v-pills-teams-tab">
            <%= render "teams" %>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
